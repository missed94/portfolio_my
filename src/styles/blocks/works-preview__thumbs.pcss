.thumbs {
  display: flex;
  position: relative;
}

.thumbs__item {
  width: 116px;
  transition: 0.2s;
  /* border: 2px solid transparent; */
  &.active,
  &:hover {
    outline: 2px solid #dc9322;
    z-index: 2;
  }
}

.thumbs__link {
}

.thumb {
  width: 100%;
  height: 100%;
  object-fit: cover;
}
